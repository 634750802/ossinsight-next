{
  "name": "@ossinsight/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run gen:endpoints && next dev",
    "build": "pnpm run sync:endpoints && pnpm run gen:endpoints && next build",
    "start": "next start",
    "lint": "next lint",
    "sync:endpoints": "./scripts/download-endpoints-configs.sh",
    "gen:d.ts": "json2ts ../schemas/data-api/v1/EndpointConfig.schema.json ./types/EndpointConfig.schema.d.ts",
    "gen:endpoints": "ossinsight endpoints bundle --endpoints-dir ./configs/endpoints --template-file ./configs/templates/endpoints.ts.liquid --output-path ./app/api/queries/[...query]/endpoints.ts"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@napi-rs/canvas": "^0.1.44",
    "@next/mdx": "13.5.2",
    "@ossinsight/widgets-core": "workspace:^",
    "@ossinsight/widgets-utils": "workspace:^",
    "@primer/octicons-react": "^19.7.0",
    "@tidbcloud/serverless": "^0.0.6",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "clsx": "^2.0.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.5.2",
    "liquidjs": "^10.9.2",
    "next": "^13.5.2",
    "next-plugin-svgr": "^1.1.8",
    "postcss": "8.4.26",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@ossinsight/cli": "workspace:^",
    "@ossinsight/ui": "workspace:^",
    "@ossinsight/widgets-types": "workspace:^",
    "@tailwindcss/typography": "^0.5.10",
    "@types/luxon": "^3.3.2",
    "@types/mdx": "^2.0.7",
    "bootstrap-icons": "^1.11.1",
    "json-schema-to-ts": "^2.9.2",
    "json-schema-to-typescript": "^13.1.1",
    "luxon": "^3.4.3",
    "raw-loader": "^4.0.2",
    "sass": "^1.68.0",
    "tailwind-merge": "^1.14.0"
  }
}
